---
---
[{% for post in site.posts %}{
  "episode_URL": "{{ post.episode_URL }}",
  "title": "{{ post.title }}",
  "date": "{{ post.date | date: "%B %d, %Y" }}",
  "category": "{{ post.category }}",
  "author": "{{ post.author }}",
  "illustrator": "{{ post.illustrator }}",
  "writer": "{{ post.writer }}",
  "guest_name": "{{ post.guest_name }}",
  "guest_color": "{{ post.guest_color }}",
  "track1_title": "{{ post.track1_title }}",
  "track1_color": "{{ post.track1_color }}",
  "track2_title": "{{ post.track2_title }}",
  "track2_color": "{{ post.track2_color }}",
  "track3_title": "{{ post.track3_title }}",
  "track3_color": "{{ post.track3_color }}",
  "track4_title": "{{ post.track4_title }}",
  "track4_color": "{{ post.track4_color }}",
  "track5_title": "{{ post.track5_title }}",
  "track5_color": "{{ post.track5_color }}",
  "track6_title": "{{ post.track6_title }}",
  "track6_color": "{{ post.track6_color }}",
  "track7_title": "{{ post.track7_title }}",
  "track7_color": "{{ post.track7_color }}",
  "track1_link": "{{ post.track1_link }}",
  "track2_link": "{{ post.track2_link }}",
  "track3_link": "{{ post.track3_link }}",
  "track4_link": "{{ post.track4_link }}",
  "track5_link": "{{ post.track5_link }}",
  "track6_link": "{{ post.track6_link }}",
  "track7_link": "{{ post.track7_link }}",
  "guestPic": "{{ post.guestPic }}",
  "image": "{{ post.image }}",
  "musiColor": "{{post.musiColor}}",
  "description": "{{ post.description | replace: '"',"" }}",
  "content": "{{ post.content | url_encode }}"
}{% unless forloop.last %},{% endunless %}{% endfor %}]